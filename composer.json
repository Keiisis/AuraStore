{
    "name": "aura/store",
    "description": "Premium AI-Powered Shopping Platform",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-gd": "*",
        "vlucas/phpdotenv": "^5.6",
        "intervention/image": "^3.0",
        "monolog/monolog": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Aura\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
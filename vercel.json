{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(login|register|dashboard|admin|store|setup-store|logout|index)\\.php",
            "dest": "/api/$1.php"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}
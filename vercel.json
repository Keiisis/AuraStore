{
    "cleanUrls": true,
    "functions": {
        "*.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/style.css",
            "dest": "/style.css"
        },
        {
            "src": "/main.js",
            "dest": "/main.js"
        },
        {
            "src": "/index.html",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}
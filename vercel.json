{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/api/uploads/(.*)",
            "dest": "/public/uploads/$1"
        },
        {
            "src": "/(login|register|dashboard|admin|store|setup-store|logout|index)\\.php",
            "dest": "/api/$1.php"
        },
        {
            "src": "/api/(.*)\\.php",
            "dest": "/api/$1.php"
        }
    ]
}
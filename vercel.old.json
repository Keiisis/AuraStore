{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "DB_HOST": "127.0.0.1",
        "DB_NAME": "aurastore",
        "DB_USER": "root",
        "DB_PASS": ""
    }
}